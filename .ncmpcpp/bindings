# Make shortcut keys more vim like

def_key "h"
  previous_column

def_key "h"
  master_screen

def_key "h"
  volume_down

def_key "j"
  scroll_down

def_key "k"
  scroll_up

def_key "l"
  next_column

def_key "l"
  slave_screen

def_key "l"
  volume_up

def_key "ctrl_b"
  page_up

def_key "ctrl_f"
  page_down


